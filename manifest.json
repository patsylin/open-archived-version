{
  "manifest_version": 3,
  "name": "Archive Reader",
  "short_name": "Archive Reader",
  "version": "0.1.0",
  "description": "Open the newest archived version of the current page via Wayback Machine (or Archive.ph).",
  "action": {
    "default_title": "Open archived version",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
