{
  "manifest_version": 3,
  "name": "Archive Reader",
  "short_name": "Archive Reader",
  "version": "0.1.0",
  "description": "description": "Open archived versions of pages instantly â€” Wayback Machine & Archive.ph support.",
    "version_name": "0.1.0",
  "author": "Patsy Lin",
  "homepage_url": "https://github.com/patsylin/open-archived-version",
  "action": {
    "default_title": "Open archived version",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "storage", "contextMenus"],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
